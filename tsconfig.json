{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@navigation/*": ["navigation/*"],
      "@utils/*": ["utils/*"],
      "@assets/*": ["assets/*"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@types/*": ["types/*"],
      "@services/*": ["services/*"],
      "@store/*": ["store/*"],
      "@theme/*": ["theme/*"],
      "@i18n/*": ["i18n/*"],
      "@config/*": ["config/*"],
      "@test/*": ["test/*"]
    }
  }
}
